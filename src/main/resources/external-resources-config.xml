<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<resource>
		<id>bootstrap</id>
		<locations>
			<location>
				<name>bootstrap-sources.tar.gz</name>
				<url>https://github.com/twitter/bootstrap/tarball/master</url>
				<filters>
					<filter>
						<pattern>.*/less/.*\.less</pattern>
						<destination>src/main/webapp/resources/theme/less/bootstrap</destination>
					</filter>
				</filters>
			</location>
			<location>
				<name>bootstrap-dist.zip</name>
				<url>http://twitter.github.com/bootstrap/assets/bootstrap.zip</url>
				<filters>
					<filter>
						<pattern>.*/js/.*\.js</pattern>
						<destination>src/main/webapp/resources/theme/js</destination>
					</filter>
					<filter>
						<pattern>.*/img/.*\.png</pattern>
						<destination>src/main/webapp/resources/theme/img</destination>
					</filter>
				</filters>
			</location>
		</locations>
	</resource>
	<resource>
		<id>jquery</id>
		<locations>
			<location>
				<name>jquery.min.js</name>
				<url>http://code.jquery.com/jquery-latest.min.js</url>
				<filters>
					<filter>
						<pattern>.*\.js</pattern>
						<destination>src/main/webapp/resources/theme/js</destination>
					</filter>
				</filters>
			</location>
		</locations>
	</resource>
	<resource>
		<id>html5shiv</id>
		<locations>
			<location>
				<name>html5shiv.zip</name>
				<url>https://github.com/aFarkas/html5shiv/zipball/master</url>
				<filters>
					<filter>
						<pattern>.*/dist/html5shiv.js</pattern>
						<destination>src/main/webapp/resources/theme/js</destination>
					</filter>
				</filters>
			</location>
		</locations>
	</resource>
</resources>